<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mp.module.mapper.BrandMapper">

    <select id="getAllByName" resultType="mp.module.entity.Brand">
        select * from base_brand where name_=#{name}
    </select>
</mapper>
